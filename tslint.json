{
  "extends": ["tslint-config-airbnb"],
  "rules": {
    "no-increment-decrement": false,
    "variable-name": false,
    "import-name": false,
    "trailing-comma": false,
    "no-duplicate-imports": false,
    "max-line-length": false,
    "ter-arrow-parens": false,
    "semicolon": false,
    "ordered-imports": true,
    "align": [false, "parameters"]
  }
}
